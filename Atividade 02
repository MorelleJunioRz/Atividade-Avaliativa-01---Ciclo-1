
int main()

{

  int num, numero1, numero2, numero3, cont;

  do {

   printf("Digite um numero:\n ");

    scanf("%i", &num);

    if(num <= 0)

    printf("Digite um numero positivo!!");

  } while (num <= 0);

  printf(" 1 - ");

  numero1 = 0; numero 2 = 1;

  num = num -2;

  for(cont = 0; cont <= num; cont++) {

    numero3 = numero2 + numero1;

    printf("%i - ", numero3);

    numero1=numero2 ; numero2 = numero3;

  }

}
